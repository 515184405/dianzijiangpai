.getUserInfo{
  height:350rpx;
  width:350rpx;
  color:#fff;
  background-color: rgba(14,23,54,1);
  border-radius: 175rpx;
  display: block;
  margin:50% auto;
  line-height: 350rpx;
}
.btn-box{
  position: fixed;
  bottom:0;
  left: 0;
  width:100%;
  height:100rpx;
  background-color: #fff;
  display: flex;
  justify-content: space-between;
  z-index: 999;
}
.shareImg{
  width:100%;
}
.btn-box button{
  width:300rpx;
  height:70rpx;
  line-height:70rpx;
  margin-top:15rpx;
  font-size: 32rpx;
  text-align: center;
  background-color: #fff;
}
.btn-box .btn1{
  border:1px solid #5181a1;
  color:#5181a1;
}
.btn-box .btn2{
  background-color: #5181a1;
  color:#fff;
}